<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ranking.model.ReflectVO" table="reflect" >
    
        <id name="no" column="no" >
			<generator class="identity"/>
        </id>

        <property name="reflectedNo" column="reflectedNo" />
        <property name="authorNo" column="authorNo" />
        <property name="reflectedDate" column="reflectedDate" />
        <property name="authorDate" column="authorDate" />
        
    </class>
</hibernate-mapping>

